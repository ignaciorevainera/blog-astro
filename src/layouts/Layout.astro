---
import "@/styles/global.css";

import BaseHead from "@/components/BaseHead.astro";

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
	<BaseHead title={title} />
	<body class="bg-dark-blue text-white">
		<main>
			<slot />
		</main>
	</body>
</html>
